<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0"
	xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"

	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="Main" transaction-type="RESOURCE_LOCAL">

		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>

		<class>edu.university.roombooking.domain.UsrGroupUsrPK</class>
		<class>edu.university.roombooking.domain.UsrGroupUsr</class>
		<class>edu.university.roombooking.domain.UsrGroup</class>
		<class>edu.university.roombooking.domain.Usr</class>
		<class>edu.university.roombooking.domain.RoomPK</class>
		<class>edu.university.roombooking.domain.RoomAttributeTypePK</class>
		<class>edu.university.roombooking.domain.RoomAttributeType</class>
		<class>edu.university.roombooking.domain.RoomAttributePK</class>
		<class>edu.university.roombooking.domain.RoomAttribute</class>
		<class>edu.university.roombooking.domain.Room</class>
		<class>edu.university.roombooking.domain.Reservation</class>
		<class>edu.university.roombooking.domain.PersonGroupPersonPK</class>
		<class>edu.university.roombooking.domain.PersonGroupPerson</class>
		<class>edu.university.roombooking.domain.PersonGroup</class>
		<class>edu.university.roombooking.domain.Person</class>
		<class>edu.university.roombooking.domain.DbStatusPK</class>
		<class>edu.university.roombooking.domain.DbStatus</class>
		<class>edu.university.roombooking.domain.RoomManagerPK</class>
		<class>edu.university.roombooking.domain.RoomManager</class>
		<class>edu.university.roombooking.domain.CampusPK</class>
		<class>edu.university.roombooking.domain.Campus</class>
		<class>edu.university.roombooking.domain.BuildingPK</class>
		<class>edu.university.roombooking.domain.Building</class>
		<class>edu.university.roombooking.domain.BookableRoomPK</class>
		<class>edu.university.roombooking.domain.BookableRoom</class>
		<class>edu.university.roombooking.domain.UsrGroupAuth</class>
		<class>edu.university.roombooking.domain.UsrGroupAuthPK</class>
		<class>edu.university.roombooking.domain.PersonGroupPK</class>
		<class>edu.university.roombooking.domain.UsrGroupPK</class>
		<class>edu.university.roombooking.domain.UsrPK</class>
		<class>edu.university.roombooking.domain.Authority</class>
		<class>edu.university.roombooking.domain.AuthorityPK</class>
		<shared-cache-mode>NONE</shared-cache-mode>
		<properties>
			<property name="eclipselink.logging.level" value="ALL" />
			<property name="javax.persistence.jdbc.url"
				value="jdbc:postgresql://localhost:5432/rb" />
			<property name="javax.persistence.jdbc.user" value="postgres" />
			<property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver" />
			<property name="eclipselink.weaving" value="false" />
			<property name="javax.persistence.jdbc.password" value="ppp" />
			<!-- <property name="eclipselink.weaving" value="true"/> -->
		</properties>
	</persistence-unit>
</persistence>
